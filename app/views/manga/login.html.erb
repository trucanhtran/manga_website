<%= form_with do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
<div>
    <%= link_to 'Sign Up', signup_path  %>
</div>
